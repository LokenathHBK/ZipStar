<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/addstore_container"
    >




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relative_store_cont"
        android:orientation="vertical"
        android:layout_below="@+id/selectstore_actionBar"

        >
        <ProgressBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:id="@+id/pbar"
            android:visibility="gone"
            />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:id="@+id/selectstore_actionBar"

            android:background="#067BC0"
            >



            <com.esolz.su.zipstar.customviews.MyriadProSemibold
                android:id="@+id/title_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="Settings"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                />



        </RelativeLayout>

        <!--lena hai layout-->


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:weightSum="8"
            android:orientation="vertical"
            android:layout_below="@+id/selectstore_actionBar"
            android:id="@+id/tab"
            >


            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:id="@+id/deliver_container"
                android:background="#ffffff"

                android:layout_weight="7"
                >

                <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical"
                android:layout_below="@+id/actionBar"
                android:weightSum="8"
                android:background="#F0F0F0"
                >


                <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:background="#ffffff"
                android:paddingLeft="25dp"
                android:id="@+id/personalSetting"
                android:layout_weight="1"
                android:gravity="center_vertical"
                    android:weightSum="1"
                >


                <com.esolz.su.zipstar.customviews.MyriadProSemibold
                android:id="@+id/Itemname"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:paddingTop="5dp"
                android:textColor="#000000"
                android:text="Personal Setting"
android:textStyle="bold"
                    android:layout_weight=".9"
                />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:src="@drawable/rightarrow"
                        android:layout_weight=".1"
                        />


                </LinearLayout>


                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:layout_gravity="center_horizontal"
                        android:background="#F0F0F0"

                        />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:background="#ffffff"
                        android:paddingLeft="25dp"
                        android:id="@+id/accountSetting"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:weightSum="1"
                        >


                        <com.esolz.su.zipstar.customviews.MyriadProSemibold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:textSize="20sp"
                            android:paddingTop="5dp"
                            android:textColor="#000000"
                            android:text="Account Setting"
                            android:textStyle="bold"
                            android:layout_weight=".9"
                            />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_alignParentLeft="true"
                            android:src="@drawable/rightarrow"
                            android:layout_weight=".1"
                            />


                    </LinearLayout>
                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:layout_gravity="center_horizontal"
                        android:background="#F0F0F0"

                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:background="#ffffff"
                        android:paddingLeft="25dp"
                        android:id="@+id/paymentinfo"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:weightSum="1"
                        >


                        <com.esolz.su.zipstar.customviews.MyriadProSemibold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:textSize="20sp"
                            android:paddingTop="5dp"
                            android:textColor="#000000"
                            android:text="Payment Info"
                            android:textStyle="bold"
                            android:layout_weight=".9"
                            />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_alignParentLeft="true"
                            android:src="@drawable/rightarrow"
                            android:layout_weight=".1"
                            />


                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:layout_gravity="center_horizontal"
                        android:background="#F0F0F0"

                        />
                <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:background="#ffffff"
                android:paddingLeft="25dp"
                android:id="@+id/notification"

                android:layout_weight="1"
                android:gravity="center_vertical"
                >
                <com.esolz.su.zipstar.customviews.MyriadProSemibold
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:paddingTop="5dp"
                android:textColor="#000000"
                android:text="Notifications"
                    android:textStyle="bold"

                />
                <com.esolz.su.zipstar.customviews.MyriadProSemibold
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:paddingTop="5dp"
                android:textColor="#DA1E00"
                android:text="("
                android:layout_marginLeft="10dp"
                />
                <com.esolz.su.zipstar.customviews.MyriadProSemibold
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:paddingTop="5dp"
                android:textColor="#DA1E00"
                android:text="23"

                />
                <com.esolz.su.zipstar.customviews.MyriadProSemibold
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:paddingTop="5dp"
                android:textColor="#DA1E00"
                android:text=")"

                />


                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/rightarrow"
                        android:layout_marginLeft="170dp"

                        />

                </LinearLayout>
                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:layout_gravity="center_horizontal"
                        android:background="#F0F0F0"

                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:background="#ffffff"
                        android:paddingLeft="25dp"
                        android:id="@+id/sendfeedback"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:weightSum="1"
                        >


                        <com.esolz.su.zipstar.customviews.MyriadProSemibold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:textSize="20sp"
                            android:paddingTop="5dp"
                            android:textColor="#000000"
                            android:text="Send Feedbacks"
                            android:textStyle="bold"
                            android:layout_weight=".9"
                            />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_alignParentLeft="true"
                            android:src="@drawable/rightarrow"
                            android:layout_weight=".1"
                            />


                    </LinearLayout>
                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:layout_gravity="center_horizontal"
                        android:background="#F0F0F0"

                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:background="#ffffff"
                        android:paddingLeft="25dp"
                        android:id="@+id/logout"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:weightSum="1"
                        >


                        <com.esolz.su.zipstar.customviews.MyriadProSemibold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:textSize="20sp"
                            android:paddingTop="5dp"
                            android:textColor="#000000"
                            android:text="Logout"
                            android:textStyle="bold"
                            android:layout_weight=".9"
                            />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_alignParentLeft="true"
                            android:src="@drawable/rightarrow"
                            android:layout_weight=".1"
                            />


                    </LinearLayout>


                <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:layout_weight="2"
                android:gravity="center_vertical"
                android:background="#F0F0F0"
                >

                </LinearLayout>


                </LinearLayout>







            </FrameLayout>



            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:id="@+id/landing_container"
                android:layout_weight="1"
                android:weightSum="4"
                android:orientation="horizontal"
                android:background="#4E4E4E"
                >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:id="@+id/one"
                    android:gravity="center"
                    >
                    <ImageButton
                        android:layout_width="35dp"
                        android:layout_height="40dp"
                        android:background="@drawable/bottombaricon1"
                        android:layout_gravity="center"
                        android:src="@drawable/landing_selector"

                        android:id="@+id/oneimg"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:id="@+id/two"
                    android:gravity="center"

                    >
                    <ImageButton
                        android:layout_width="35dp"
                        android:layout_height="40dp"
                        android:background="@drawable/bottombaricon2"
                        android:layout_gravity="center"
                        android:id="@+id/twoimg"
                        android:src="@drawable/landing_selector"

                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:id="@+id/three"
                    android:gravity="center"
                    >
                    <ImageButton
                        android:id="@+id/threeimg"
                        android:src="@drawable/landing_selector"

                        android:layout_width="35dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center"
                        android:background="@drawable/bottombaricon3"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:id="@+id/four"
                    android:gravity="center"
                    >
                    <ImageButton
                        android:id="@+id/fourimg"

                        android:layout_width="35dp"
                        android:layout_height="40dp"
                        android:layout_alignParentLeft="true"
                        android:background="@drawable/bottombaricon4"
                        android:src="@drawable/landing_selector"

                        android:layout_gravity="center"
                        />
                </LinearLayout>


            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</FrameLayout>